local ToggleButton = Instance.new("TextButton")
ToggleButton.BorderSizePixel = 0
ToggleButton.BackgroundColor3 = Color3.fromRGB(200, 50, 50) -- Start with red (off)
ToggleButton.Position = UDim2.new(0, 5, 0, 2)
ToggleButton.Size = UDim2.new(0, 20, 0, 20)
ToggleButton.Text = ""
ToggleButton.Parent = AutoDeleteToggle

local ToggleLabel = Instance.new("TextLabel")
ToggleLabel.BorderSizePixel = 0
ToggleLabel.BackgroundTransparency = 1
ToggleLabel.Position = UDim2.new(0, 30, 0, 0)
ToggleLabel.Size = UDim2.new(1, -35, 1, 0)
ToggleLabel.Text = "Auto Delete"
ToggleLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
ToggleLabel.TextSize = 14
ToggleLabel.TextXAlignment = Enum.TextXAlignment.Left
ToggleLabel.Parent = AutoDeleteToggle

local autoDeleteEnabled = false
local autoDeleteConnection = nil

-- Create rarity tabs and content frames
petRarities = {"Common", "Uncommon", "Rare", "Epic", "Legendary"}
auraRarities = {"Common", "Uncommon", "Rare", "Epic", "Legendary"}

-- Create pet rarity tabs
petRarityTabs = {}
auraRarityTabs = {}

-- Function to show the appropriate rarity tabs
local function ShowRarityTabs(contentType)
    -- Hide all tabs first
    for _, child in pairs(RarityTabsFrame:GetChildren()) do
        if child:IsA("TextButton") then
            child.Visible = false
        end
    end
    
    -- Show only the tabs for the current content type
    if contentType == "Pet" then
        for rarity, tab in pairs(petRarityTabs) do
            tab.Visible = true
        end
    else
        for rarity, tab in pairs(auraRarityTabs) do
            tab.Visible = true
        end
    end
end

-- Create pet and aura rarity tabs
petRarityTabs = CreateRarityTabs(RarityTabsFrame, petRarities, "Pet")
auraRarityTabs = CreateRarityTabs(RarityTabsFrame, auraRarities, "Aura")

-- Initially show only pet tabs
ShowRarityTabs("Pet")

-- Create content frames
petContentFrames = CreateRarityContentFrames(PetsContent, petRarities, "Pet")
auraContentFrames = CreateRarityContentFrames(AurasContent, auraRarities, "Aura")

-- Update status indicator with count of selected pets
local function UpdateStatusIndicator()
    local selectedPets = GetSelectedPets()
    local petCount = 0
    local auraCount = 0
    
    for _, pet in ipairs(selectedPets) do
        if pet.type == "Pet" then
            petCount = petCount + 1
        else
            auraCount = auraCount + 1
        end
    end
    
    StatusIndicator.Text = "Selected: " .. petCount .. " pets, " .. auraCount .. " auras"
end

-- Function to create a pet button
local function CreatePetButton(petName, petData)
    local petButton = Instance.new("Frame")
    petButton.Name = petName:gsub("%s+", "_") -- Name the frame exactly after the pet, replacing spaces with underscores
    petButton.Size = UDim2.new(1, -10, 0, 40)
    petButton.BackgroundColor3 = Color3.fromRGB(60, 60, 60)
    petButton.BorderSizePixel = 0
    
    local petNameLabel = Instance.new("TextLabel")
    petNameLabel.Name = "PetNameLabel" -- Add a name to make it easier to find
    petNameLabel.Size = UDim2.new(0.7, 0, 1, 0)
    petNameLabel.Position = UDim2.new(0, 5, 0, 0)
    petNameLabel.BackgroundTransparency = 1
    petNameLabel.Text = petName -- Set the correct pet name
    petNameLabel.TextColor3 = Color3.new(1, 1, 1)
    petNameLabel.TextSize = 14
    petNameLabel.TextXAlignment = Enum.TextXAlignment.Left
    petNameLabel.Parent = petButton
    
    -- Set color based on rarity
    local rarityColor
    if petData.rarity == "Common" then
        rarityColor = Color3.fromRGB(255, 255, 255)
    elseif petData.rarity == "Uncommon" then
        rarityColor = Color3.fromRGB(100, 255, 100)
    elseif petData.rarity == "Rare" then
        rarityColor = Color3.fromRGB(100, 170, 255)
    elseif petData.rarity == "Epic" then
        rarityColor = Color3.fromRGB(170, 100, 255)
    elseif petData.rarity == "Legendary" then
        rarityColor = Color3.fromRGB(255, 215, 0)
    end
    
    local rarityLabel = Instance.new("TextLabel")
    rarityLabel.Size = UDim2.new(0.3, 0, 0.4, 0)
    rarityLabel.Position = UDim2.new(0.7, 0, 0, 0)
    rarityLabel.BackgroundTransparency = 1
    rarityLabel.Text = petData.rarity
    rarityLabel.TextColor3 = rarityColor
    rarityLabel.TextSize = 12
    rarityLabel.Parent = petButton
    
    local petImageContainer = Instance.new("Frame")
    petImageContainer.Size = UDim2.new(0.3, -10, 0.6, 0)
    petImageContainer.Position = UDim2.new(0.7, 5, 0.4, 0)
    petImageContainer.BackgroundTransparency = 1
    petImageContainer.ClipsDescendants = true
    petImageContainer.Parent = petButton
    
    local petImage = Instance.new("ImageLabel")
    petImage.Size = UDim2.new(1, 0, 1, 0)
    petImage.BackgroundTransparency = 1
    petImage.Image = petData.id
    petImage.ScaleType = Enum.ScaleType.Fit
    petImage.Parent = petImageContainer
    
    local selectButton = Instance.new("TextButton")
    selectButton.Size = UDim2.new(1, 0, 1, 0)
    selectButton.BackgroundTransparency = 1
    selectButton.Text = ""
    selectButton.Parent = petButton
    
    selectButton.MouseButton1Click:Connect(function()
        -- Add functionality for when a pet is selected
        print("Selected pet: " .. petName)
        
        -- Create a new frame for the selected pet with the correct name
        local selectedPet = Instance.new("Frame")
        selectedPet.Name = petName:gsub("%s+", "_") -- Name the selected pet frame exactly after the pet
        selectedPet.Size = UDim2.new(1, -10, 0, 40)
        selectedPet.BackgroundColor3 = Color3.fromRGB(60, 60, 60)
        selectedPet.BorderSizePixel = 0
        
        -- Create a new label with the correct pet name
        local selectedPetNameLabel = Instance.new("TextLabel")
        selectedPetNameLabel.Size = UDim2.new(0.7, 0, 1, 0)
        selectedPetNameLabel.Position = UDim2.new(0, 5, 0, 0)
        selectedPetNameLabel.BackgroundTransparency = 1
        selectedPetNameLabel.Text = petName -- Use the correct pet name
        selectedPetNameLabel.TextColor3 = Color3.new(1, 1, 1)
        selectedPetNameLabel.TextSize = 14
        selectedPetNameLabel.TextXAlignment = Enum.TextXAlignment.Left
        selectedPetNameLabel.Parent = selectedPet
        
        -- Add rarity label
        local selectedRarityLabel = Instance.new("TextLabel")
        selectedRarityLabel.Size = UDim2.new(0.3, 0, 0.4, 0)
        selectedRarityLabel.Position = UDim2.new(0.7, 0, 0, 0)
        selectedRarityLabel.BackgroundTransparency = 1
        selectedRarityLabel.Text = petData.rarity
        selectedRarityLabel.TextColor3 = rarityColor
        selectedRarityLabel.TextSize = 12
        selectedRarityLabel.Parent = selectedPet
        
        -- Add a remove button to the selected pet
        local removeButton = Instance.new("TextButton")
        removeButton.Size = UDim2.new(0, 20, 0, 20)
        removeButton.Position = UDim2.new(1, -25, 0, 10)
        removeButton.BackgroundColor3 = Color3.fromRGB(200, 50, 50)
        removeButton.Text = "X"
        removeButton.TextColor3 = Color3.new(1, 1, 1)
        removeButton.Parent = selectedPet
        
        removeButton.MouseButton1Click:Connect(function()
            selectedPet:Destroy()
            UpdateStatusIndicator() -- Update the count when removed
        end)
        
        selectedPet.Parent = SelectedPetsList
        UpdateStatusIndicator() -- Update the count when added
    end)
    
    return petButton
end


-- Function to populate pet lists
local function PopulatePetLists()
    -- Populate pet lists for each rarity
    for petName, petData in pairs(PetNames) do
        if petData.type == "Pet" then
            rarityFrame = petContentFrames[petData.rarity]
            if rarityFrame then
                petList = rarityFrame:FindFirstChild("PetList")
                if petList then
                    petButton = CreatePetButton(petName, petData)
                    petButton.Parent = petList
                end
            end
        elseif petData.type == "Aura" then
            rarityFrame = auraContentFrames[petData.rarity]
            if rarityFrame then
                petList = rarityFrame:FindFirstChild("PetList")
                if petList then
                    petButton = CreatePetButton(petName, petData)
                    petButton.Parent = petList
                end
            end
        end
    end
end

-- Function to handle tab switching for rarities
local function SetupRarityTabSwitching(tabs, contentFrames, contentType)
    for rarity, tab in pairs(tabs) do
        tab.MouseButton1Click:Connect(function()
            -- Update tab appearance
            for _, otherTab in pairs(tabs) do
                otherTab.BackgroundColor3 = Color3.fromRGB(40, 40, 40)
            end
            tab.BackgroundColor3 = Color3.fromRGB(60, 60, 60)
            
            -- Show the correct content frame
            for otherRarity, contentFrame in pairs(contentFrames) do
                contentFrame.Visible = (otherRarity == rarity)
            end
        end)
    end
end

-- Setup main tab switching (Pets/Auras)
PetsTab.MouseButton1Click:Connect(function()
    PetsTab.BackgroundColor3 = Color3.fromRGB(60, 60, 60)
    AurasTab.BackgroundColor3 = Color3.fromRGB(40, 40, 40)
    PetsContent.Visible = true
    AurasContent.Visible = false
    ShowRarityTabs("Pet")
end)

AurasTab.MouseButton1Click:Connect(function()
    AurasTab.BackgroundColor3 = Color3.fromRGB(60, 60, 60)
    PetsTab.BackgroundColor3 = Color3.fromRGB(40, 40, 40)
    PetsContent.Visible = false
    AurasContent.Visible = true
    ShowRarityTabs("Aura")
end)

-- Setup rarity tab switching
SetupRarityTabSwitching(petRarityTabs, petContentFrames, "Pet")
SetupRarityTabSwitching(auraRarityTabs, auraContentFrames, "Aura")

-- Make the main frame draggable
UserInputService = game:GetService("UserInputService")
local dragging
local dragInput
local dragStart
local startPos

local function update(input)
    delta = input.Position - dragStart
    MainFrame.Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y)
end

TopBar.InputBegan:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
        dragging = true
        dragStart = input.Position
        startPos = MainFrame.Position
        
        input.Changed:Connect(function()
            if input.UserInputState == Enum.UserInputState.End then
                dragging = false
            end
        end)
    end
end)

TopBar.InputChanged:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
        dragInput = input
    end
end)

UserInputService.InputChanged:Connect(function(input)
    if input == dragInput and dragging then
        update(input)
    end
end)

-- Connect buttons
Exit.MouseButton1Click:Connect(function()
    MainScreen:Destroy()
end)

Minimize.MouseButton1Click:Connect(function()
    if PetSelectFrame.Visible then
        PetSelectFrame.Visible = false
        MainFrame.Size = UDim2.new(0, 500, 0, 30)
    else
        PetSelectFrame.Visible = true
        MainFrame.Size = UDim2.new(0, 500, 0, 350)
    end
end)

