    -- Show help information
    helpFrame = Instance.new("Frame")
    helpFrame.Size = UDim2.new(0, 300, 0, 200)
    helpFrame.Position = UDim2.new(0.5, -150, 0.5, -100)
    helpFrame.BackgroundColor3 = Color3.fromRGB(40, 40, 40)
    helpFrame.BorderSizePixel = 0
    helpFrame.Parent = MainScreen
    
    helpTitle = Instance.new("TextLabel")
    helpTitle.Size = UDim2.new(1, 0, 0, 30)
    helpTitle.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
    helpTitle.Text = "Help"
    helpTitle.TextColor3 = Color3.new(1, 1, 1)
    helpTitle.TextSize = 16
    helpTitle.Parent = helpFrame
    
    helpText = Instance.new("TextLabel")
    helpText.Size = UDim2.new(1, -20, 1, -40)
    helpText.Position = UDim2.new(0, 10, 0, 35)
    helpText.BackgroundTransparency = 1
    helpText.Text = "This tool helps you auto-delete unwanted pets in Pet Ranch Simulator.\n\n1. Select pets you want to auto-delete by clicking on them\n2. Use the tabs to navigate between different rarities and pet types\n3. Click 'Save Selection' to confirm your choices"
    helpText.TextColor3 = Color3.new(1, 1, 1)
    helpText.TextSize = 14
    helpText.TextWrapped = true
    helpText.TextXAlignment = Enum.TextXAlignment.Left
    helpText.TextYAlignment = Enum.TextYAlignment.Top
    helpText.Parent = helpFrame
    
    closeButton = Instance.new("TextButton")
    closeButton.Size = UDim2.new(0, 80, 0, 25)
    closeButton.Position = UDim2.new(0.5, -40, 1, -30)
    closeButton.BackgroundColor3 = Color3.fromRGB(60, 60, 60)
    closeButton.Text = "Close"
    closeButton.TextColor3 = Color3.new(1, 1, 1)
    closeButton.Parent = helpFrame
    
    closeButton.MouseButton1Click:Connect(function()
        helpFrame:Destroy()
    end)
end)


local function GetSelectedPets()
    local selectedPets = {}
    
    for _, child in pairs(SelectedPetsList:GetChildren()) do
        if child:IsA("Frame") then
            local petNameLabel = child:FindFirstChildOfClass("TextLabel")
            if petNameLabel and petNameLabel.Text and PetNames[petNameLabel.Text] then
                table.insert(selectedPets, {
                    name = petNameLabel.Text,
                    id = PetNames[petNameLabel.Text].id,
                    type = PetNames[petNameLabel.Text].type
                })
            end
        end
    end
    
    return selectedPets
end

-- Updated CheckAndDeletePets function to properly navigate the UI structure
local function CheckAndDeletePets()
    local selectedPets = GetSelectedPets()
    if #selectedPets == 0 then 
        print("No pets selected for auto-delete")
        return 
    end
    
    print("Checking for pets to auto-delete. Selected count: " .. #selectedPets)
    
    -- Check pet inventory
    local petInventoryPath = game:GetService("Players").LocalPlayer.PlayerGui.MainGui.CenterUIFrame.PetInventory.PetInventoryInnerFrame.NewGui.MainFrame2.Pets.PetInventoryScrollingFrame
    if petInventoryPath then
        print("Found pet inventory path")
        for _, petFrame in pairs(petInventoryPath:GetChildren()) do
            if petFrame:IsA("ImageButton") then
                local petImage = petFrame:FindFirstChild("PetImage")
                if petImage and petImage:IsA("ImageButton") then
                    -- Get the image ID
                    local imageId = petImage.Image
                    
                    -- Debug output
                    print("Found pet with image: " .. imageId)
                    
                    -- Check if this pet's image matches any selected pet
                    for _, selectedPet in ipairs(selectedPets) do
                        if selectedPet.type == "Pet" and imageId == selectedPet.id then
                            -- Get the pet ID from the frame name
                            local petId = petFrame.Name
                            
                            print("MATCH FOUND! Deleting pet: " .. selectedPet.name .. " (ID: " .. petId .. ")")
                            
                            -- Delete the pet
                            local args = {
                                "DeletePetInInventory",
                                petId
                            }
                            game:GetService("ReplicatedStorage"):WaitForChild("RemoteEvents"):WaitForChild("MainRemoteEvent"):FireServer(unpack(args))
                            
                            -- Add a small delay to prevent server overload
                            wait(0.1)
                        end
                    end
                end
            end
        end
    else
        print("Could not find pet inventory path")
    end
    
    -- Check aura inventory
    local auraInventoryPath = game:GetService("Players").LocalPlayer.PlayerGui.MainGui.CenterUIFrame.PetInventory.AuraInventoryInnerFrame.ScreenGui.AuraFrame.Auras.AuraInventoryScrollingFrame
    if auraInventoryPath then
        print("Found aura inventory path")
        for _, auraFrame in pairs(auraInventoryPath:GetChildren()) do
            -- Only process frames that could contain auras (skip UI elements)
            if auraFrame:IsA("ImageButton") then
                local auraImage = auraFrame:FindFirstChild("PetImage")
                if auraImage and auraImage:IsA("ImageButton") then
                    -- Get the image ID
                    local imageId = auraImage.Image
                    
                    -- Debug output
                    print("Found aura with image: " .. imageId)
                    
                    -- Check if this aura's image matches any selected aura
                    for _, selectedPet in ipairs(selectedPets) do
                        if selectedPet.type == "Aura" and imageId == selectedPet.id then
                            -- Get the aura ID from the frame name
                            local auraId = auraFrame.Name
                            
                            print("MATCH FOUND! Deleting aura: " .. selectedPet.name .. " (ID: " .. auraId .. ")")
                            
                            -- Delete the aura
                            local args = {
                                "DeleteAuraInInventory",
                                auraId
                            }
                            game:GetService("ReplicatedStorage"):WaitForChild("RemoteEvents"):WaitForChild("MainRemoteEvent"):FireServer(unpack(args))
                            
                            -- Add a small delay to prevent server overload
                            wait(0.1)
                        end
                    end
                end
            end
        end
    else
        print("Could not find aura inventory path")
    end
end


-- Improved toggle function with better error handling
ToggleButton.MouseButton1Click:Connect(function()
    autoDeleteEnabled = not autoDeleteEnabled
    
    if autoDeleteEnabled then
        -- Turn on auto delete
        ToggleButton.BackgroundColor3 = Color3.fromRGB(60, 180, 60) -- Green (on)
        
        -- Show confirmation
        local confirmationLabel = Instance.new("TextLabel")
        confirmationLabel.Size = UDim2.new(1, -10, 0, 25)
        confirmationLabel.Position = UDim2.new(0, 5, 1, -60)
        confirmationLabel.BackgroundColor3 = Color3.fromRGB(60, 180, 60)
        confirmationLabel.Text = "Auto Delete Enabled!"
        confirmationLabel.TextColor3 = Color3.new(1, 1, 1)
        confirmationLabel.Parent = SelectedPetsFrame
        game:GetService("Debris"):AddItem(confirmationLabel, 3)
        
        print("Auto Delete enabled - starting check loop")
        
        -- Start the auto delete loop
        if autoDeleteConnection then
            autoDeleteConnection:Disconnect()
        end
        
        -- Run an immediate check
        spawn(function()
            wait(0.1) -- Wait a moment for UI to update
            CheckAndDeletePets()
        end)
        
        autoDeleteConnection = game:GetService("RunService").Heartbeat:Connect(function()
            -- Only check every 3 seconds to reduce performance impact
            if tick() % 0.1 < 0.1 then
                spawn(function()
                    pcall(function()
                        print("Running auto-delete check...")
                        CheckAndDeletePets()
                    end)
                end)
            end
        end)
    else
        -- Turn off auto delete
        ToggleButton.BackgroundColor3 = Color3.fromRGB(200, 50, 50) -- Red (off)
        
        -- Show confirmation
        local confirmationLabel = Instance.new("TextLabel")
        confirmationLabel.Size = UDim2.new(1, -10, 0, 25)
        confirmationLabel.Position = UDim2.new(0, 5, 1, -60)
        confirmationLabel.BackgroundColor3 = Color3.fromRGB(200, 50, 50)
        confirmationLabel.Text = "Auto Delete Disabled!"
        confirmationLabel.TextColor3 = Color3.new(1, 1, 1)
        confirmationLabel.Parent = SelectedPetsFrame
        game:GetService("Debris"):AddItem(confirmationLabel, 3)
        
        print("Auto Delete disabled")
        
        -- Stop the auto delete loop
        if autoDeleteConnection then
            autoDeleteConnection:Disconnect()
            autoDeleteConnection = nil
        end
    end
end)

-- Connect to ChildAdded and ChildRemoved events to update the status
SelectedPetsList.ChildAdded:Connect(UpdateStatusIndicator)
SelectedPetsList.ChildRemoved:Connect(UpdateStatusIndicator)

PopulatePetLists()
