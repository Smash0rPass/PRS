-- Function to create rarity tabs with specific positions
local function CreateRarityTabs(parent, rarities, contentType)
    local tabs = {}
    
    -- Define specific positions for each rarity
    local positions = {
        ["Common"] = UDim2.new(0, 0, 0, -100),
        ["Uncommon"] = UDim2.new(0.2, 0, 0, -100),
        ["Rare"] = UDim2.new(0.4, 0, 0, -100),
        ["Epic"] = UDim2.new(0.6, 0, 0, -100),
        ["Legendary"] = UDim2.new(0.8, 0, 0, -100)
    }
    
    for i, rarity in ipairs(rarities) do
        local tab = Instance.new("TextButton")
        tab.BorderSizePixel = 0
        tab.BackgroundColor3 = (i == 1) and Color3.fromRGB(60,60,60) or Color3.fromRGB(40,40,40)
        
        -- Use the specific position for this rarity
        tab.Position = positions[rarity]
        tab.Size = UDim2.new(0.2, 0, 0, 30) -- Fixed width of 0.2 (20% of parent)
        tab.Text = rarity
        tab.Name = contentType .. rarity .. "Tab"
        
        -- Set color based on rarity
        local textColor
        if rarity == "Common" then
            textColor = Color3.fromRGB(255, 255, 255)
        elseif rarity == "Uncommon" then
            textColor = Color3.fromRGB(100, 255, 100)
        elseif rarity == "Rare" then
            textColor = Color3.fromRGB(100, 170, 255)
        elseif rarity == "Epic" then
            textColor = Color3.fromRGB(170, 100, 255)
        elseif rarity == "Legendary" then
            textColor = Color3.fromRGB(255, 215, 0)
        else
            textColor = Color3.fromRGB(255, 255, 255)
        end
        
        tab.TextColor3 = textColor
        tab.TextSize = 14
        tab.Parent = parent
        tabs[rarity] = tab
    end
    
    return tabs
end


-- Create pet content frames for each rarity
local function CreateRarityContentFrames(parent, rarities, contentType)
    contentFrames = {}
    
    for i, rarity in ipairs(rarities) do
        local frame = Instance.new("Frame")
        frame.BorderSizePixel = 0
        frame.BackgroundTransparency = 1
        frame.Position = UDim2.new(0, 0, 0, 0)
        frame.Size = UDim2.new(1, 0, 1, 0)
        frame.Visible = (i == 1)
        frame.Name = contentType .. rarity .. "Content"
        frame.Parent = parent
        contentFrames[rarity] = frame
        
        -- Add pet list (adjusted position and size since search bar is removed)
        petList = Instance.new("ScrollingFrame")
        petList.Name = "PetList"
        petList.BorderSizePixel = 0 
        petList.BackgroundColor3 = Color3.fromRGB(50,50,50)
        petList.Position = UDim2.new(0, 10, 0, 10) -- Changed from 50 to 10
        petList.Size = UDim2.new(0.6, -15, 1, -20) -- Changed from -60 to -20
        petList.AutomaticCanvasSize = Enum.AutomaticSize.Y
        petList.ScrollBarThickness = 6
        petList.Parent = frame
        
        petListUIList = Instance.new("UIListLayout")
        petListUIList.Padding = UDim.new(0, 5)
        petListUIList.FillDirection = Enum.FillDirection.Vertical
        petListUIList.HorizontalAlignment = Enum.HorizontalAlignment.Left
        petListUIList.VerticalAlignment = Enum.VerticalAlignment.Top
        petListUIList.Parent = petList
    end
    
    return contentFrames
end


