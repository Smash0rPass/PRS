local tweenInfo = TweenInfo.new(
    0.5, -- Duration
    Enum.EasingStyle.Quad,
    Enum.EasingDirection.Out
)

RebirthSettings.MouseButton1Click:Connect(function()
    if not isFrameExpanded then
        -- Show and expand the frame
        RebirthFrame.Visible = true
        local expandTween = TweenService:Create(RebirthFrame, tweenInfo, {
            Position = UDim2.new(0, 460, 0, 0)
        })
        expandTween:Play()
        isFrameExpanded = true
    else
        -- Collapse and hide the frame
        local collapseTween = TweenService:Create(RebirthFrame, tweenInfo, {
            Position = UDim2.new(0, 230, 0, 0)
        })
        collapseTween:Play()
        
        -- Wait for tween to complete before hiding
        collapseTween.Completed:Connect(function()
            RebirthFrame.Visible = false
        end)
        isFrameExpanded = false
    end
end)


