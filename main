local Players = game:GetService("Players")
local isRunning = false

local function fireAllButtonSignals(button)
    local signals = {"MouseButton1Down"}
    for _, signal in pairs(signals) do
        firesignal(button[signal])
    end
end

local function clickBubbleButtons()
    local bubbleGuis = Players.LocalPlayer.PlayerGui.BubbleGuis:GetChildren()
    
    for _, gui in ipairs(bubbleGuis) do
        if gui:IsA("BillboardGui") and gui.Enabled then
            -- Fire signals for any ImageButton or TextButton found in the BillboardGui
            for _, button in pairs(gui:GetDescendants()) do
                if button:IsA("ImageButton") or button:IsA("TextButton") then
                    fireAllButtonSignals(button)
                end
            end
        end
    end
end

AutoCollectToggle.MouseButton1Click:Connect(function()
    isRunning = not isRunning
    AutoCollectToggle.BackgroundColor3 = isRunning and Color3.fromRGB(100, 178, 115) or Color3.fromRGB(160, 55, 55)
    
    while isRunning do
        clickBubbleButtons()
        task.wait(1)
    end
end)

local notificationCount = 0
local notifications = {}

local function createNotification()
    notificationCount = notificationCount + 1
    
    local NotificationFrame = Instance.new("Frame")
    NotificationFrame.Name = "NotificationFrame" .. notificationCount
    NotificationFrame.Size = UDim2.new(0, 300, 0, 50)
    NotificationFrame.Position = UDim2.new(1, 0, 1, -50)
    NotificationFrame.BackgroundColor3 = Color3.fromRGB(45, 45, 45)
    NotificationFrame.BorderSizePixel = 0
    NotificationFrame.Parent = PetRanchSimulator

    local NotificationText = Instance.new("TextLabel")
    NotificationText.Size = UDim2.new(1, -10, 1, 0)
    NotificationText.Position = UDim2.new(0, 5, 0, 0)
    NotificationText.Text = "Requires to be in the middle to work otherwise will kick you."
    NotificationText.TextColor3 = Color3.fromRGB(255, 255, 255)
    NotificationText.BackgroundTransparency = 1
    NotificationText.TextSize = 14
    NotificationText.TextWrapped = true
    NotificationText.Parent = NotificationFrame

    local NotificationCorner = Instance.new("UICorner")
    NotificationCorner.CornerRadius = UDim.new(0, 8)
    NotificationCorner.Parent = NotificationFrame

    table.insert(notifications, 1, NotificationFrame)
    
    -- Adjust positions of all notifications
    for i, notif in ipairs(notifications) do
        local tweenInfo = TweenInfo.new(0.5, Enum.EasingStyle.Quad, Enum.EasingDirection.Out)
        local showTween = TweenService:Create(notif, tweenInfo, {Position = UDim2.new(1, -300, 1, -50 - (55 * (i-1)))})
        showTween:Play()
    end
    
    -- Remove notification after delay
    task.delay(3, function()
        local hideTween = TweenService:Create(NotificationFrame, TweenInfo.new(0.5), {Position = UDim2.new(1, 0, NotificationFrame.Position.Y.Scale, NotificationFrame.Position.Y.Offset)})
        hideTween:Play()
        
        hideTween.Completed:Wait()
        table.remove(notifications, table.find(notifications, NotificationFrame))
        NotificationFrame:Destroy()
        
        -- Readjust remaining notifications
        for i, notif in ipairs(notifications) do
            TweenService:Create(notif, TweenInfo.new(0.3), {Position = UDim2.new(1, -300, 1, -50 - (55 * (i-1)))})
            :Play()
        end
    end)
end

AutoChest.MouseButton1Click:Connect(function()
    local player = game.Players.LocalPlayer
    local character = player.Character
    if not character then return end
    
    local rootPart = character:FindFirstChild("HumanoidRootPart")
    if not rootPart then return end
    
    local distance = (rootPart.Position - Vector3.new(48, 321, 32)).Magnitude

    if distance > 100 then
        createNotification()
        return
    end

    isAutoChest = not isAutoChest
    AutoChest.BackgroundColor3 = isAutoChest and Color3.fromRGB(100, 178, 115) or Color3.fromRGB(160, 55, 55)
    
    spawn(function()
        while isAutoChest do
            local player = game:GetService("Players").LocalPlayer
            local character = player.Character
            if character then
                local humanoidRootPart = character:FindFirstChild("HumanoidRootPart")
                if humanoidRootPart then
                    local originalPosition = humanoidRootPart.Position
                    local timers = player.PlayerGui.LocalTimers
                    local foundChest = false
                    
                    for _, timer in pairs(timers:GetChildren()) do
                        if timer:IsA("IntValue") and timer.Value == 0 then
                            -- Check door colors for rebirth chests
                            local canTeleport = true
                            if timer.Name == "RebirthChest1" then
                                local door = workspace.RebirthDoor1.Door
                                canTeleport = (door.Color == Color3.fromRGB(0, 255, 0))
                            elseif timer.Name == "RebirthChest2" then
                                local door = workspace.RebirthDoor2.Door
                                canTeleport = (door.Color == Color3.fromRGB(0, 255, 0))
                            elseif timer.Name == "RebirthChest3" then
                                local door = workspace.RebirthDoor3.Door
                                canTeleport = (door.Color == Color3.fromRGB(0, 255, 0))
                            end
                            
                            if canTeleport then
                                local chest = workspace.TreasureChests:FindFirstChild(timer.Name)
                                if chest then
                                    foundChest = true
                                    humanoidRootPart.CFrame = chest.CFrame
                                    task.wait(2)
                                end
                            end
                        end
                    end
                    
                    if foundChest then
                        humanoidRootPart.CFrame = CFrame.new(originalPosition)
                    end
                end
            end
            task.wait(0.1)
        end
    end)
end)


local WalkspeedCons = {
    wsLoop = nil,
    wsCA = nil
}

Walkspeed.MouseButton1Click:Connect(function()
    isWalkspeed = not isWalkspeed
    Walkspeed.BackgroundColor3 = isWalkspeed and Color3.fromRGB(100, 178, 115) or Color3.fromRGB(160, 55, 55)
    
    if isWalkspeed then
        local player = game:GetService("Players").LocalPlayer
        local speed = 30
        
        local function WalkSpeedChange()
            local char = player.Character
            local human = char and char:FindFirstChildWhichIsA("Humanoid")
            if char and human then
                human.WalkSpeed = speed
            end
        end
        
        WalkSpeedChange()
        
        WalkspeedCons.wsLoop = player.Character:FindFirstChildWhichIsA("Humanoid"):GetPropertyChangedSignal("WalkSpeed"):Connect(WalkSpeedChange)
        WalkspeedCons.wsCA = player.CharacterAdded:Connect(function(nChar)
            WalkspeedCons.wsLoop:Disconnect()
            WalkSpeedChange()
            WalkspeedCons.wsLoop = nChar:WaitForChild("Humanoid"):GetPropertyChangedSignal("WalkSpeed"):Connect(WalkSpeedChange)
        end)
    else
        if WalkspeedCons.wsLoop then
            WalkspeedCons.wsLoop:Disconnect()
            WalkspeedCons.wsLoop = nil
        end
        if WalkspeedCons.wsCA then
            WalkspeedCons.wsCA:Disconnect()
            WalkspeedCons.wsCA = nil
        end
        
        local human = game:GetService("Players").LocalPlayer.Character:FindFirstChildWhichIsA("Humanoid")
        if human then
            human.WalkSpeed = 16
        end
    end
end)


-- Function to create or update range visualization
local function updateRangeVisualization(center, enabled)
    if not enabled then
        if rangeVisualization then
            rangeVisualization:Destroy()
            rangeVisualization = nil
        end
        return
    end

    if not rangeVisualization then
        rangeVisualization = Instance.new("Part")
        rangeVisualization.Name = "RangeVisualization"
        rangeVisualization.Anchored = true
        rangeVisualization.CanCollide = false
        rangeVisualization.Transparency = 0.9
        rangeVisualization.Color = Color3.fromRGB(255, 255, 255)
        rangeVisualization.Material = Enum.Material.Neon
        rangeVisualization.Parent = workspace
        rangeVisualizationCylinder = Instance.new("CylinderMesh")
        rangeVisualizationCylinder.Parent = rangeVisualization
    end

    rangeVisualization.Size = Vector3.new(15, 0.2, 15) -- Circle with 10 studs radius (X/Z size), flat on Y
    rangeVisualization.Position = center
end

AutoHatch.MouseButton1Click:Connect(function()
    isAutoHatching = not isAutoHatching
    AutoHatch.BackgroundColor3 = isAutoHatching and Color3.fromRGB(100, 178, 115) or Color3.fromRGB(160, 55, 55)

    local player = Players.LocalPlayer
    local statDisplay = game:GetService("ReplicatedStorage"):WaitForChild(player.Name.."StatDisplay")
    local hasTripleEgg = statDisplay:WaitForChild("Gamepass_Buy3Eggs").Value

    while isAutoHatching do
        local targetModels = {
            "Tier1EggCapsule", "Tier2EggCapsule", "Tier3EggCapsule", "Tier4EggCapsule",
            "Tier5EggCapsule", "Tier6EggCapsule", "Tier7EggCapsule", "Tier8EggCapsule",
            "Tier9EggCapsule", "Tier10EggCapsule", "AuraBox1", "RebirthAuraBox1",
            "RebirthAuraBox2", "RebirthAuraBox3", "RebirthEggCapsule",
            "AdvancedRebirthEggCapsule", "UltimateRebirthEggCapsule",
            "GoldyRebirthEggCapsule"
        }

        local character = player.Character

        if character then
            local humanoidRootPart = character:FindFirstChild("HumanoidRootPart")
            if humanoidRootPart then
                updateRangeVisualization(humanoidRootPart.Position, isAutoHatching)

                local closestDistance = math.huge
                local closestModel = nil

                for _, modelName in ipairs(targetModels) do
                    local model = workspace:FindFirstChild(modelName)
                    if model then
                        local referencePart
                        for _, part in ipairs(model:GetDescendants()) do
                            if part:IsA("BasePart") then
                                referencePart = part
                                break
                            end
                        end

                        if referencePart then
                            local distance = Vector3.new(
                                humanoidRootPart.Position.X - referencePart.Position.X,
                                0,
                                humanoidRootPart.Position.Z - referencePart.Position.Z
                            ).Magnitude

                            if distance < 7 and distance < closestDistance then
                                closestDistance = distance
                                closestModel = model
                            end
                        end
                    end
                end

                if closestModel then
                    local eggType = closestModel.Name:gsub("EggCapsule", "Egg"):gsub("Capsule", "")
                    
                    if hasTripleEgg then
                        local args = {
                            [1] = "BuyThreeEggs",
                            [2] = eggType
                        }
                        game:GetService("ReplicatedStorage"):WaitForChild("RemoteFunctions")
                            :WaitForChild("MainRemoteFunction"):InvokeServer(unpack(args))
                    else
                        local args = {
                            [1] = "BuyEgg",
                            [2] = eggType
                        }
                        game:GetService("ReplicatedStorage"):WaitForChild("RemoteFunctions")
                            :WaitForChild("MainRemoteFunction"):InvokeServer(unpack(args))
                    end
                end
            end
        end
        task.wait(0.01)
    end

    updateRangeVisualization(nil, false)
end)



AutoRebirth.MouseButton1Click:Connect(function()
    isAutoRebirth = not isAutoRebirth
    AutoRebirth.BackgroundColor3 = isAutoRebirth and Color3.fromRGB(100, 178, 115) or Color3.fromRGB(160, 55, 55)

    -- Start the auto rebirth loop
    spawn(function()
        while isAutoRebirth do
            local rebirthAmount = 0
            
            if isRebirth1 then rebirthAmount = 1
            elseif isRebirth2 then rebirthAmount = 3
            elseif isRebirth3 then rebirthAmount = 5
            elseif isRebirth4 then rebirthAmount = 10
            elseif isRebirth5 then rebirthAmount = 20
            elseif isRebirth6 then rebirthAmount = 50
            elseif isRebirth7 then rebirthAmount = 150
            elseif isRebirth8 then rebirthAmount = 300
            elseif isRebirth9 then rebirthAmount = 600
            elseif isRebirth10 then rebirthAmount = 1000
            elseif isRebirth11 then rebirthAmount = 2500
            elseif isRebirth12 then rebirthAmount = 5000
            elseif isRebirth13 then rebirthAmount = 15000
            elseif isRebirth14 then rebirthAmount = 50000
            elseif isRebirth15 then rebirthAmount = 200000
            end

            if rebirthAmount > 0 then
                local args = {
                    [1] = "RebirthPlayer",
                    [2] = rebirthAmount
                }
                game:GetService("ReplicatedStorage"):WaitForChild("RemoteFunctions"):WaitForChild("MainRemoteFunction"):InvokeServer(unpack(args))
            end
            
            wait(2)
        end
    end)
end)



function CommonDeletus()
    local petInventory = game.Players.LocalPlayer.PlayerGui.MainGui.CenterUIFrame.PetInventory.PetInventoryInnerFrame.PetInventoryScrollingFrame
    for _, v in pairs(petInventory:GetChildren()) do 
        if v:IsA("ImageButton") and v:FindFirstChild("PetImage") then
            for i = 1, #CommonIds do
                if v.PetImage.Image == CommonIds[i] then
					mre:FireServer("DeletePetInInventory", v.Name)
                end
            end
        end
    end
end

