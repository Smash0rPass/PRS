--Function for roundedCorners
local function addRoundedCorners(instance, radius, radius2)
    local corner = Instance.new("UICorner")
    corner.CornerRadius = UDim.new(radius, radius2)
    corner.Parent = instance
end

-- Frames with radius 0.05
addRoundedCorners(MainFrame, 0.05)
addRoundedCorners(RebirthFrame, 0.05)
addRoundedCorners(PetsDelete, 0.05)
addRoundedCorners(AuraDelete, 0.05)
addRoundedCorners(PetsTitleFrame, 0, 6)
addRoundedCorners(AurasTitleFrame, 0, 6)


-- Exit button with radius 2
addRoundedCorners(Exit, 2)

-- All other buttons with radius 0.3
addRoundedCorners(AutoEquip, 0.3)
addRoundedCorners(AutoRebirth, 0.3)
