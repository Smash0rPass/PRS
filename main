Equip.MouseButton1Click:Connect(function()
    -- Get the pet inventory
    local petInventory = game:GetService("Players").LocalPlayer.PlayerGui.MainGui.CenterUIFrame.PetInventory.PetInventoryInnerFrame.PetInventoryScrollingFrame

    -- State to track if any pets were destroyed
    local petsDestroyed = false

    -- Check each pet
    for _, v in pairs(petInventory:GetChildren()) do
        if v:IsA("ImageButton") then
            -- Check if the pet has the specific colors
            if v.ImageColor3 == Color3.new(255/255, 255/255, 127/255) or v.ImageColor3 == Color3.new(135/255, 255/255, 183/255) then
                petsDestroyed = true -- Set state to true if a pet is destroyed
                local destroyArgs = {
                    [1] = "DestroyPet",
                    [2] = v.Name
                }
                mrf:InvokeServer(unpack(destroyArgs))
                wait(0.01) -- Small delay between each destroy call
            end
        end
    end

    -- Only fire the EquipTopPets remote if at least one pet was destroyed
    if petsDestroyed then
        wait(0.5) -- Short delay to ensure all DestroyPet calls are processed
        local equipArgs = {
            [1] = "EquipTopPets"
        }
        mrf:InvokeServer(unpack(equipArgs))
    end
end)

AutoEquip.MouseButton1Click:Connect(function()
    isAutoEquip = not isAutoEquip
    AutoEquip.BackgroundColor3 = isAutoEquip and Color3.fromRGB(100, 178, 115) or Color3.fromRGB(160, 55, 55)
    
    spawn(function()
        while isAutoEquip do
            local petsEquipped = game:GetService("Players").LocalPlayer.PlayerGui.MainGui.CenterUIFrame.PetInventory.PetEquipped.Text
            local maxPetsEquipped = game:GetService("Players").LocalPlayer.PlayerGui.MainGui.CenterUIFrame.PetInventory.MaxPetEquipped.Text
            
            if petsEquipped ~= maxPetsEquipped then
                local equipArgs = {
                    [1] = "EquipTopPets"
                }
                mrf:InvokeServer(unpack(equipArgs))
            end
            
            task.wait(0.01)
        end
    end)
end)



AutoUpgRanch.MouseButton1Click:Connect(function()
    isAutoUpgRanch = not isAutoUpgRanch
    AutoUpgRanch.BackgroundColor3 = isAutoUpgRanch and Color3.fromRGB(100, 178, 115) or Color3.fromRGB(160, 55, 55)
    
    spawn(function()
        while isAutoUpgRanch do
            local upgradeText = game:GetService("Players").LocalPlayer.PlayerGui.MainGui.CenterUIFrame.UpgradeRanchFrame.DragonCapAfter.Text
            
            if upgradeText ~= "Max Upgrade" then
                local args = {
                    [1] = "UpgradeRanch",
                    [2] = false
                }
                mrf:InvokeServer(unpack(args))
            end
            
            task.wait()
        end
    end)
end)


