-- Billboard GUI size, distance, and transparency adjustment
local function adjustBillboardGuis()
    local bubbleGuis = game:GetService("Players").LocalPlayer.PlayerGui:WaitForChild("BubbleGuis")
    for _, gui in pairs(bubbleGuis:GetChildren()) do
        if gui:IsA("BillboardGui") then
            gui.MaxDistance = 2000
            
            -- Adjust ImageButtons inside the BillboardGui
            for _, child in pairs(gui:GetDescendants()) do
                if child:IsA("ImageButton") then
                    child.ImageTransparency = 0
                end
            end
        end
    end
end

-- Run initial adjustment
adjustBillboardGuis()

-- Set up connection to handle new BillboardGuis
game:GetService("Players").LocalPlayer.PlayerGui:WaitForChild("BubbleGuis").ChildAdded:Connect(function(child)
    if child:IsA("BillboardGui") then
        child.MaxDistance = 2000
        
        -- Adjust ImageButtons inside the new BillboardGui
        for _, descendant in pairs(child:GetDescendants()) do
            if descendant:IsA("ImageButton") then
                descendant.ImageTransparency = 0
            end
        end
    end
end)


    end
